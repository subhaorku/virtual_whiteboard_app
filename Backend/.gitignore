# A gitignore file for a Node.js project, that also ignores .DS_Store files, build artifacts, and IDE configurations.# Node.js dependencies in every subfolder


# Node modules
node_modules/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Directory for instrumented libs generated by jscoverage/JSCover
lib-cov

# Coverage directory used by tools like Istanbul
coverage/

# nyc test coverage
.nyc_output/

# Grunt intermediate storage
.grunt/

# Bower dependency directory (if used)
bower_components/

# Compiled binary addons (node-gyp)
build/Release

# IDE settings
.vscode/
.idea/

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Mac system files
.DS_Store
**/.DS_Store

# Output of 'npm pack'
*.tgz

# Parcel-bundler cache (if using parcel)
.cache/

# Next.js build output (if used)
.next/

# SvelteKit build output (if used)
.svelte-kit/

# Express-generated public assets (if dynamically generated)
public/build/
